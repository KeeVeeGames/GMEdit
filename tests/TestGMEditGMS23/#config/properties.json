{
    "indentWithTabs": true,
    "linterPrefs": {
        "addMissingArgsToJSDoc": true,
        "implicitBoolIntCasts": false,
        "requireFields": true,
        "specTypeColon": true,
        "specTypeConst": true,
        "specTypeInst": true,
        "specTypeInstSubTopLevel": true,
        "specTypeLet": true,
        "specTypeVar": false,
        "strictScriptSelf": true,
        "strictStaticJSDoc": true,
        "warnAboutRedundantCasts": true
    },
    "nullConditionalSet": "nc_set",
    "nullConditionalVal": "nc_val",
    "privateFieldRegex": "^__",
    "privateGlobalRegex": "^__"
}